{
  "name": "npm-module-path",
  "version": "2.0.1",
  "description": "Local and global search module directory",
  "license": "MIT",
  "repository": "mrmlnc/npm-module-path",
  "author": {
    "name": "Denis Malinochkin",
    "url": "canonium.com"
  },
